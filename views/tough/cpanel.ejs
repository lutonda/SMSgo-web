  <script src="/socket.io/socket.io.js"></script>
  <script>
      $(document).ready(function(){
        $('a.js-colorlib-nav-toggle.colorlib-nav-toggle.active').trigger('click').on('click',()=>{location.href='/'})

        var socket = io.connect('http://localhost:9600');
        $('div.card').attr('class','card')
        <% user.stations.forEach((station)=>{%>
        socket.emit('server-device-check', {deviceId:'<%= station.deviceId %>'});
        socket.on('device-<%= station.deviceId %>-active!',(data)=>{
            $('#<%= station.deviceId %>').attr('class','card active')
        });
        socket.on('device-<%= station.deviceId %>-not-active!',(data)=>{
            debugger
            $('#<%= station.deviceId %>').attr('class','card')
        });
        <% }) %>
      })
  </script>
